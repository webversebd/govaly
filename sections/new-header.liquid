  {% schema %}
  {
    "name": "New Header",
    "tag": "section",
    "class": "section",
    "settings": [
      {
        "type": "color_scheme",
        "id": "color_scheme",
        "label": "t:sections.all.colors.label",
        "default": "scheme-1"
      },
      {
        "type": "header",
        "content": "t:sections.all.padding.section_padding_heading"
      },
      {
        "type": "range",
        "id": "padding_top",
        "min": 0,
        "max": 100,
        "step": 4,
        "unit": "px",
        "label": "t:sections.all.padding.padding_top",
        "default": 40
      },
      {
        "type": "range",
        "id": "padding_bottom",
        "min": 0,
        "max": 100,
        "step": 4,
        "unit": "px",
        "label": "t:sections.all.padding.padding_bottom",
        "default": 52
      },
      {
      "type": "link_list",
      "id": "menu",
      "default": "main-menu",
      "label": "Left menu"
    },
    {
      "type": "link_list",
      "id": "middle_menu",
      "default": "main-menu",
      "label": "Middle menu"
    },
    {
      "type": "link_list",
      "id": "right_menu",
      "default": "main-menu",
      "label": "Right menu"
    },
    {
      "type": "link_list",
      "id": "mobile_menu",
      "default": "main-menu",
      "label": "Mobile menu"
    }
    ],
    "presets": [
      {
        "name": "New Header"
      }
    ]
  }
  {% endschema %}
  